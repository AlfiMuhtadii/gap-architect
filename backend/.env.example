APP_ENV=development
APP_NAME=gap-architect
API_V1_STR=/api/v1
DEBUG=true

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=gap_architect
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
DATABASE_URL=
ASYNC_DATABASE_URL=

CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

USE_ESCO=false
ESCO_SKILLS_CSV=backend/datasets/skill_en.csv
CANONICAL_SKILLS_PATH=backend/datasets/canonical_skills.txt
ESCO_CACHE_TTL_SECONDS=300

LLM_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com
OPENAI_MODEL=gpt-4o-mini
LLM_TIMEOUT_SECONDS=30

LOCAL_LLM_BASE_URL=http://localhost:11434
LOCAL_LLM_MODEL=llama3.1
LOCAL_LLM_TIMEOUT_SECONDS=300

MAX_RESUME_CHARS=20000
MAX_JD_CHARS=20000
MAX_PROMPT_CHARS=60000

EMBEDDING_PROVIDER=none
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_SIM_THRESHOLD=0.55
EMBEDDING_CANDIDATE_LIMIT=250

TRANSLATE_ENABLED=false
TRANSLATE_TARGET_LANG=en

REDIS_URL=
RATE_LIMIT_PER_MINUTE=120
RATE_LIMIT_MAX_KEYS=10000
RATE_LIMIT_KEY_HEADER=
